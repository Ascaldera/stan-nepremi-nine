<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--\\\\\\\\\\\\\\\\\\\\\\\\\\  CONTACTS: dodaja polj  \\\\\\\\\\\\\\\\\\-->
    <record id="res_partner_form_inherited" model="ir.ui.view"> 
        <field name="name">Inherited Res Partner</field> 
        <field name="model">res.partner</field> 
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml"> 
            
            <xpath expr="/form/sheet/div[2]/div[1]" position="after">
                <group col='2'>
                    <field name="tip_stranke"/>
                    <field name="tretja_oseba"/>
                </group>
            </xpath>

            
            <!-- Contact TAB: Info o nepremičninah -->
            <xpath expr="//page[@name='sales_purchases']" position='after'>
                <page name='nepremicnine' string='Nepremičnine' attrs="{'invisible':[('tretja_oseba','=',False),('tip_stranke','not in',['prodajalec', 'najemodajalec'])]}">
                    <group string='Podatki o nepremičninah'>
                        <group col='4' colspan='2'>
                            <field name="pridobitev" attrs="{'invisible': [('tip_stranke','!=','prodajalec')]}"/>
                            <span colspan='2'/>
                            <field name="stranka_odziv"/>
                            <span colspan='2'/>
                            <field name="povprasevanje" colspan='4'/>
                            <field name="mnenje" colspan='4'/>
                        </group>
                    </group>
                    <group string='Seznam vseh nepremičnin'>   
                        <field name="nepremicnine" nolabel="1">
                            <tree editable="bottom">
                                <field name="currency_id" invisible="1"/>
                                <field name='name' string='Nepremičnina'/>
                                <field name='type' string='Tip'/>
                                <field name='list_price' string = 'Cena' widget='monetary' options="{'currency_field': 'currency_id'}"/>
                            </tree>
                        </field>  
                    </group>
                </page>
            </xpath>
            
            <!-- Contact TAB: Prodajalec -->
            <xpath expr="//page[@name='nepremicnine']" position='after'>
                <page name='prodajalec' string="Prodajalec" attrs="{'invisible': [('tip_stranke','!=','prodajalec')]}">
                    <group string='Podatki o oglasu'>
                        <group col='4' colspan='2'>
                            <field name="objava_kje"/>
                            <field name="objava_odziv"/>
                            <field name="objava_kje_datum" attrs="{'readonly':[('objava_kje','=', False)]}"/>
                            <field name="objava_spremembe"/>
                            <field name="objava_stevilo_ogledov"/>
                        </group>
                    </group>
                    <group string='Prodajalec'>        
                        <group col='4' colspan='2'>
                            <field name="create_uid" string='Osebo vnesla'/>
                            <field name="custom_currency_id" invisible="1"/>
                            <field name="prodajalec_koliko" widget='monetary' options="{'currency_field': 'custom_currency_id'}"/>
                            <field name="prodajalec_ponudbe"/>
                            
                            <label for='prodajalec_procent'/>
                            <div class="o_row" name="procent">
                                <field name="prodajalec_procent"/>
                                    <span>%</span>
                            </div>                        
                            
                            <field name="prodajalec_ponudbe_info" attrs ="{'readonly':[('prodajalec_ponudbe','=', False)]}"/>
                            <field name="prodajalec_datum_pogodbe"/>
                            <field name="prodajalec_komunikacija"/>
                        </group>
                    </group>
                </page>
            </xpath>
            
            <!-- Contact TAB: Kupci-->
            <xpath expr="//page[@name='nepremicnine']" position='after'>
                <page name='kupec' string="Kupec" attrs="{'invisible': [('tip_stranke','!=','kupec')]}">     
                    <group string='Iskalni podatki'>
                        <group col='4' colspan='2'>
                            <field name="poizvedba_cena" widget='monetary' options="{'currency_field': 'custom_currency_id'}"/>
                            <field name="custom_currency_id" invisible="1"/>
                            <field name="poizvedba_kontakt_dan"/>
                            <field name="poizvedba_kontaktiran"/>
                            <field name="poizvedba_nepremicnina" widget="many2many_tags" options="{'color_field': 'color'}" colspan='4'/>
                            <field name="poizvedba_kraj" string='Kje išče' widget="many2many_tags" options="{'color_field': 'color'}" colspan='4'/>
                            <field name="poizvedba_namen" string='S kakšnim namenom' widget="many2many_tags" options="{'color_field': 'color'}" colspan='4'/>
                            <field name="poizvedba_zgodovina" colspan='4'/>
                            <field name="poizvedba_ok" colspan='4'/>
                            <field name="poizvedba_nok" colspan='4'/>
                        </group>
                    </group> 
                    <group string='Kupec'>
                        <group col='8' colspan='2'>
                            <field name="create_uid" string = 'Osebo vnesla'/> <span colspan='6'/>
                            <field name="kupec_narocnik"/>
                            <field name="kupec_aktiven"/> <span colspan='4'/>
                            <field name="kupec_znizana_cena" colspan='4'/> <span colspan='4'/>
                            <field name="kupec_obvescanje" string='O kakšnih ponudbah ga obveščamo' widget="many2many_tags" options="{'color_field': 'color'}" colspan='8'/>
                        </group>
                    </group>
                </page>
            </xpath>
            
            
            <!-- Contact TAB: Najemodajalec-->
            <xpath expr="//page[@name='nepremicnine']" position='after'>
                <page name='najemodajalec' string="Najemodajalec" attrs="{'invisible': [('tip_stranke','!=','najemodajalec')]}">
                    <group string='Podatki o oglasu'>
                        <group col='4' colspan='2'>
                            <field name="objava_kje"/>
                            <field name="objava_odziv"/>
                            <field name="objava_kje_datum" attrs="{'readonly':[('objava_kje','=', False)]}"/>
                            <field name="objava_spremembe"/>
                            <field name="objava_stevilo_ogledov"/>
                        </group>
                    </group>
                    <group string='Najemodajalec'>
                        <field name="create_uid" string = 'Osebo vnesla'/>
                        <field name="najemodajalec_komu" string='Komu oddajajo' widget="many2many_tags" options="{'color_field': 'color'}"/>
                        <field name="najemodajalec_od"/>
                        <field name="najemodajalec_do"/>
                        <field name="najemodajalec_otroci"/>
                        <field name="najemodajalec_studenti"/>
                        <field name="najemodajalec_zivali"/>
                    </group>
                </page>
            </xpath>
            
            <!-- Contact TAB: Najemnik-->
            <xpath expr="//page[@name='nepremicnine']" position='after'>
                <page name='najemnik' string="Najemnik" attrs="{'invisible': [('tip_stranke','!=','najemnik')]}">
                    <group string='Iskalni podatki'>
                        <group col='4' colspan='2'>
                            <field name="poizvedba_cena" widget='monetary' options="{'currency_field': 'custom_currency_id'}"/>
                            <field name="custom_currency_id" invisible="1"/>
                            <field name="poizvedba_kontakt_dan"/>
                            <field name="poizvedba_kontaktiran"/>
                            <field name="poizvedba_nepremicnina" widget="many2many_tags" options="{'color_field': 'color'}" colspan='4'/>
                            <field name="poizvedba_kraj" string='Kje išče' widget="many2many_tags" options="{'color_field': 'color'}" colspan='4'/>
                            <field name="poizvedba_namen" string='S kakšnim namenom' widget="many2many_tags" options="{'color_field': 'color'}" colspan='4'/>
                            <field name="poizvedba_zgodovina" colspan='4'/>
                            <field name="poizvedba_ok" colspan='4'/>
                            <field name="poizvedba_nok" colspan='4'/>
                        </group>
                    </group>
                    <group string='Najemnik'>
                        <group col='4' colspan='2'>
                            <field name="create_uid" string = 'Osebo vnesla' colspan='4'/>
                            
                            <field name="najemnik_osebe"/>
                            <field name="najemnik_dolzina"/>
                            
                            <field name="najemnik_zgodovina"/>
                            <span colspan='2'/>
                            
                            <field name="najemnik_zaposlitev"/>
                            <span colspan='2'/>
                        </group>
                        <group col='6' colspan='2'>    
                            <field name="poizvedba_balkon"/>
                            <field name="poizvedba_dvigalo"/>
                            <field name="poizvedba_vrt"/>
                            <field name="najemnik_studenti"/>
                            <field name="najemnik_zivali"/>
                        </group>
                    </group>
                </page>
            </xpath>
            
            <!-- Contact TAB: Tretja Oseba-->
            <xpath expr="//page[@name='nepremicnine']" position='after'>
                <page name='tretjaoseba' string="Tretja oseba" attrs="{'invisible': ['|',('tretja_oseba','=', False),('tip_stranke', 'in', ['prodajalec','kupec','najemodajalec','najemnik'])]}">
                    <group string='Podatki o oglasu'>
                        <group col='4' colspan='2'>
                            <field name="objava_kje"/>
                            <field name="objava_odziv"/>
                            <field name="objava_kje_datum" attrs="{'readonly':[('objava_kje','=', False)]}"/>
                            <field name="objava_spremembe"/>
                            <field name="objava_stevilo_ogledov"/>
                        </group>
                    </group>
                    <group string='Tretja Oseba'>
                        <group col='4' colspan='2'>
                            <field name="create_uid" string='Osebo vnesla'/>
                            <field name="custom_currency_id" invisible="1"/>
                            
                            <field name="spol"/>
                            <field name="tretja_oseba_osebnost" string='Osebnost' widget="many2many_tags" options="{'color_field': 'color'}"/>
          
                            <field name="prodajalec_koliko" widget='monetary' options="{'currency_field': 'custom_currency_id'}"/>
                            <field name="prodajalec_ponudbe"/>
                            
                            <label for='prodajalec_procent'/>
                            <div class="o_row" name="procent">
                                <field name="prodajalec_procent"/>
                                    <span>%</span>
                            </div>                        
                            
                            <field name="prodajalec_ponudbe_info" attrs ="{'readonly':[('prodajalec_ponudbe','=', False)]}"/>
                            <field name="prodajalec_datum_pogodbe"/>
                            <field name="prodajalec_komunikacija"/>
                        </group>

                        <group col='8' colspan='2'>
                            <field name="kupec_narocnik"/>
                            <field name="kupec_aktiven"/> <span colspan='4'/>
                            <field name="kupec_znizana_cena" colspan='4'/> <span colspan='4'/>
                            <field name="tretja_oseba_obvescanje" string='O kakšnih ponudbah ga obveščamo' widget="many2many_tags" options="{'color_field': 'color'}" colspan='8'/>
                        </group>
                    </group>
                </page>
            </xpath>
            
        </field>
    </record>
    
    <!--\\\\\\\\\\\\\\\\\\\\\\\\\\  INVENTORY: TAG CONFIG  \\\\\\\\\\\\\\\\\\-->
    <record id="res_partner_category_inherited" model="ir.ui.view"> 
        <field name="name">Inherited Config Tags Form</field> 
        <field name="model">res.partner.category</field> 
        <field name="inherit_id" ref="base.view_partner_category_form"/> 
        <field name="arch" type="xml"> 
            <field name='name' position='after'>
                <field name="tag_type"/>
                <field name="color"/>
            </field> 
        </field>
    </record>

    
    <!--\\\\\\\\\\\\\\\\\\\\\\\\\\  INVENTORY: dodaja polj  \\\\\\\\\\\\\\\\\\-->
    <record id="res_product_form_inherited" model="ir.ui.view"> 
        <field name="name">Inherited Product Template</field> 
        <field name="model">product.template</field> 
        <field name="inherit_id" ref="product.product_template_form_view"/> 
        <field name="arch" type="xml"> 
            <!-- CUSTOM TAB -->
            <xpath expr="//page[@name='general_information']" position='after'>
                <page name='nepremicnina' string="Nepremičnina">
                    <group>
                        <group string='Lokacija'>
                            <label for="street" string="Naslov"/>
                            <div class="o_address_format">
                                <field name="nepremicnina_lokacija" class="o_address_city" placeholder='Ulica'/>
                                <div>
                                    <field name="nepremicnina_obcina" class="o_address_city" placeholder='Občina' options="{&quot;no_open&quot;: True}"/>
                                    <field name="nepremicnina_zip" class="o_address_zip" placeholder='Poštna številka'/>
                                </div>
                                <field name="nepremicnina_drzava" class="o_address_country" placeholder='Država' options="{&quot;no_open&quot;: True, &quot;no_create&quot;: True}"/>
                            </div>
                            
                            <field name="nepremicnina_regija" placeholder='Regija'/>
                            <field name="nepremicnina_soseska" placeholder='Soseska'/>
                            <field name="nepremicnina_lokacija_opombe" placeholder='Opombe'/>
                        </group>
                        <group string='Osnovne informacije'>
                            <field name="currency_id" invisible="1"/>
                            <field name="create_uid" string = 'Nepremičnino vnesla'/>

                            <label for='nepremicnina_povrsina'/>
                            <div class="o_row" name="povrsina">
                                <field name="nepremicnina_povrsina"/>
                                    <span>m²</span>
                            </div>

                            <label for='nepremicnina_zemljisce_pod'/>
                            <div class="o_row" name="povrsina_zemljisce_pod">
                                <field name="nepremicnina_zemljisce_pod"/>
                                    <span>m²</span>
                            </div>

                            <label for='nepremicnina_velikost'/>
                            <div class="o_row" name="povrsina_skupna">
                                <field name="nepremicnina_velikost"/>
                                    <span>m²</span>
                            </div>

                            <field name="nepremicnina_cena_min" widget='monetary' options="{'currency_field': 'currency_id'}"/>
                            <field name="nepremicnina_cena_dolgorocno" widget='monetary' options="{'currency_field': 'currency_id'}"/>

                            <field name="nepremicnina_vrsta"/>
                            <field name="nepremicnina_vrsta_tip_drugo" attrs ="{'invisible':[('nepremicnina_vrsta','!=', 'drugo')]}"/>
                            <field name="nepremicnina_tip"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
    
